        "comment.block.xml",
        "punctuation.definition.comment.yaml",
        "comment.line.number-sign.yaml"
      ],
      "settings": {
        "foreground": "#7A7E85"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.parameter",
        "variable.other.readwrite",
        "variable.other.object",
        "string.unquoted.plain.out.yaml"
